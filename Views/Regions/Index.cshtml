@{
    ViewData["Title"] = "Index";
}
@model IEnumerable<NPWalksUI.Models.DTO.RegionDto>

<h1 class="text text-center">Response</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Name</th>
            <th>Image</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var walk in Model)
        {
                <tr>
                    <td>@walk.Id</td>
                    <td>@walk.Code</td>
                    <td>@walk.Name</td>
                    <td><img src=@walk.RegionImageUrl /></td>
                </tr>
        }
    </tbody>
</table>